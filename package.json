{
  "name": "before-dawn",
  "productName": "Before Dawn",
  "version": "0.9.9",
  "description": "A desktop screensaver app using web technologies",
  "author": "Colin Mitchell <colin@muffinlabs.com> (http://muffinlabs.com)",
  "license": "MIT",
  "homepage": "https://github.com/muffinista/before-dawn/",
  "main": "app/main.js",
  "scripts": {
    "start": "electron app/main.js",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "test": "mocha test test/**",
    "test-lib": "nyc --reporter=html mocha test test/lib/**",
    "release": "grunt webpack && grunt sass && yarn dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/muffinista/before-dawn.git"
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "bootstrap": "4.0.0-beta",
    "electron-log": "^2.2.3",
    "ffi": "^2.2.0",
    "folder-walker": "^3.1.0",
    "fs-extra": "^2.0.0",
    "jquery": "^3.1.1",
    "linux-battery": "^3.0.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "nconf": "^0.8.5",
    "node-system-idle-time": "muffinista/node-system-idle-time.git#master",
    "noty": "^3.1.3",
    "osx-battery": "gillstrom/osx-battery.git",
    "popper.js": "^1.12.5",
    "prop-types": "^15.6.0",
    "raven": "^2.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "ref": "^1.3.4",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "robotjs": "^0.4.7",
    "semver": "^5.3.0",
    "temp": "^0.8.3",
    "yauzl": "^2.7.0"
  },
  "#depnotes": [
    "nodobjc is optional because it won't compile on windows, but is needed on OSX.",
    "however, ref is a dependency of nodobjc that won't be rebuilt by electron-rebuild",
    "unless specified as a dep so that's why it is in the rebuild command."
  ],
  "optionalDependencies": {
    "nodobjc": "^2.1.0",
    "winctl": "muffinista/winctl"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "devtron": "^1.4.0",
    "electron": "1.8.1",
    "electron-builder": "19.37.2",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "expose-loader": "^0.7.3",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-mocha-test": "^0.13.3",
    "grunt-sass": "^2.0.0",
    "grunt-webpack": "^3.0.2",
    "gruntify-eslint": "^4.0.0",
    "install": "^0.10.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "sinon": "^4.0.2",
    "spectron": "^3.7.2",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "build": {
    "appId": "Before Dawn",
    "mac": {
      "category": "public.app-category.entertainment"
    },
    "nsis": {
      "installerIcon": "build/icon.ico",
      "perMachine": false
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 338,
          "y": 14,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 14,
          "type": "file"
        }
      ]
    },
    "linux": {
      "target": "deb",
      "executableName": "before-dawn",
      "maintainer": "Colin Mitchell <colin@muffinlabs.com>"
    }
  }
}

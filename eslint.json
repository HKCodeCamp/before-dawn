{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential"],
  "rules": {
    "strict": 0
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "spread": true
    }
  },
  "plugins": [
  ],
  "rules": {
    "eqeqeq": "off",
    "curly": "warn",
    "quotes": ["warn", "double"]
  }
}

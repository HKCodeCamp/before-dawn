{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "strict": 0
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "spread": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "eqeqeq": "off",
    "curly": "warn",
    "quotes": ["warn", "double"]
  }
}
